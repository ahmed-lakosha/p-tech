            <field name="name">Company Privacy Policy</field>
            <field name="is_published">True</field>
            <field name="url">/privacy-policy</field>
            <field name="track">False</field>
            <field name="type">qweb</field>
            <field name="key">theme_ptech.company_privacy_page</field>
            <field name="is_homepage">False</field>

            <field name="arch" type="xml">
                <t name="Company Privacy Policy" priority="21"
